<!DOCTYPE HTML>
<html>
<head>
<meta charset="UTF-8" />
<title>{% block title %}Ecommerce{% endblock %}</title>
{% block stylesheets %}
        {# uncomment the following lines to compile SCSS assets with Assetic

            {% stylesheets filter="scssphp" output="css/app.css"
                "%kernel.root_dir%/Resources/assets/scss/bootstrap.scss"
                "%kernel.root_dir%/Resources/assets/scss/font-awesome.scss"
                "%kernel.root_dir%/Resources/assets/css/*.css"
                "%kernel.root_dir%/Resources/assets/scss/main.scss"
            %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        #}

            <link rel="stylesheet" href="{{ asset('css/app.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body id="{% block body_id %}{% endblock %}">

{% block header %}
<header>
<div>
 <h1>{{ 'help.more_information'|trans }}</h1>
<a href="{{ path('cadastra') }}">Cadastro</a>
</div>
</header>
{% endblock %}

</body>
</html>